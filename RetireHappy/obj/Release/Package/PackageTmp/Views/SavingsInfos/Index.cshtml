@model IEnumerable<RetireHappy.Models.SavingsInfo>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.calcRetSavings)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.savePercent)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.riskLevel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.expPercent)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.diffPercent)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.calcRetSavings)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.savePercent)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.riskLevel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.expPercent)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.diffPercent)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
